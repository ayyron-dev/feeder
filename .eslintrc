{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [
      1,
      2
    ],
    "no-underscore-dangle": [
      0
    ],
    "react/jsx-indent": [
      1,
      2
    ],
    "react/jsx-indent-props": [
      2,
      2
    ],
    "react/jsx-filename-extension": [
      0
    ],
    "react/jsx-first-prop-new-line": [
      0
    ],
    "react/prop-types": [
      0
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "max-len": [
      1,
      120
    ],
    "no-alert": [
      0
    ],
    "no-trailing-spaces": [
      0
    ],
    "no-param-reassign": [
      0
    ],
    "linebreak-style": [
      0
    ],
    "jsx-a11y/img-has-alt": [
      0
    ]
  }
}